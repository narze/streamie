{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "run-p dev watch",
    "dev": "node .",
    "watch": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "printer",
  "author": "Manassarn Manoonchai",
  "module": "dist/printer.esm.js",
  "size-limit": [
    {
      "path": "dist/printer.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/printer.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/usb": "^2.0.5",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "canvas": "^2.9.1",
    "crc-32": "^1.2.2",
    "pngjs": "^6.0.0",
    "sharp": "^0.30.3",
    "socket.io-client": "^4.4.1",
    "text2png": "^2.3.0",
    "usb": "^2.3.1"
  }
}
